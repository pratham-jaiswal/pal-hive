{
  "expo": {
    "name": "Pal Hive",
    "slug": "pal-hive",
    "scheme": "pal-hive",
    "version": "0.6.0",
    "orientation": "portrait",
    "description": "Easily track daily expenses in multiple languages and currencies.",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#A0DEFF"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#A0DEFF"
      },
      "package": "com.maxxdevs.palhive",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-auth0",
        {
          "domain": "prathamj-dev.jp.auth0.com"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": false,
            "enableShrinkResourcesInReleaseBuilds": false
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "pratham-jaiswal",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "90d08e8b-8a56-4f8f-9cdb-f7a749a3df73"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/90d08e8b-8a56-4f8f-9cdb-f7a749a3df73"
    }
  }
}
